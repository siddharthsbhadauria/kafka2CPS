# Create connection string using UAMI
    conn_str = f"host={host} dbname={database} user=UAMI@{uami_id} sslmode=require"

    # Connect to PostgreSQL
    with psycopg2.connect(conn_str) as conn:
        with conn.cursor() as cursor:
            # Modify the query as per your table schema
            query = sql.SQL("INSERT INTO your_table (column1, column2) VALUES (%s, %s);")
            cursor.execute(query, (value1, value2))  # Replace with actual values

    print(f"Blob processed successfully. Inserted into PostgreSQL.")
